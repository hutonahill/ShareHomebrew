{
  "manifest_version": 3,
  "name": "Sharing Homebrew",
  "version": "0.1",
  "description": "For Sharing D&D Beyond homebrew between accounts.",
  "icons": {
    "48": "html/logo.png"
  },
  "action": {
    "default_icon": "html/logo.png"
  },
  "permissions": [
    "activeTab", "scripting"
  ],
  "background": {
    "service_worker": "background.js",
    "type":"module"
  }
}


  
  
  