{
  "manifest_version": 3,
  "name": "Sharing Homebrew",
  "version": "0.1",
  "description": "For Sharing D&D Beyond homebrew between accounts.",
  "icons": {
    "48": "html/logo.png"
  },
  "action": {
    "default_icon": "html/logo.png",
    "default_popup": "html/wrongPage/wrongPage.html"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "service_worker": "html/background.js"
  }
}


  
  
  